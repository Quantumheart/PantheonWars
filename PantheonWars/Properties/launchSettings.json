{
  "profiles": {
    "Vintage Story Client": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)\" --addOrigin \"$(ProjectDir)/assets\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Vintage Story Server": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/VintagestoryServer.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)\" --addOrigin \"$(ProjectDir)/assets\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
