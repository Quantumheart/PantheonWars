{
  "profiles": {
    "Vintage Story Client": {
      "commandName": "Executable",
      "executablePath": "$(VINTAGE_STORY)/Vintagestory.exe",
      "commandLineArgs": "--addModPath \"$(ProjectDir)bin/$(Configuration)\" --addOrigin \"$(ProjectDir)assets\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    },
    "Vintage Story Server": {
      "commandName": "Executable",
      "executablePath": "$(VINTAGE_STORY)/VintagestoryServer.exe",
      "commandLineArgs": "--addModPath \"$(ProjectDir)bin/$(Configuration)\" --addOrigin \"$(ProjectDir)assets\" --dataPath \"$(ProjectDir).server\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
