{
  "profiles": {
    "Vintage Story Client": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/Vintagestory.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/mod\" -c localhost:42420",
      "workingDirectory": "$(VINTAGE_STORY)",
      "environmentVariables": {
        "TEXTURE_DEBUG_DISPOSE": "0",
        "VAO_DEBUG_DISPOSE": "0"
      }
    },
    "Vintage Story Server": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "\"$(VINTAGE_STORY)/VintagestoryServer.dll\" --tracelog --addModPath \"$(ProjectDir)/bin/$(Configuration)/Mods/mod\"",
      "workingDirectory": "$(VINTAGE_STORY)"
    }
  }
}
